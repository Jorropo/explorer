<!DOCTYPE html>
<html lang="en">
<head>
	<meta charset="UTF-8">
	<meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no, initial-scale=1, maximum-scale=1, user-scalable=no, minimal-ui">
	<meta http-equiv="X-UA-Compatible" content="ie=edge">

	<title>
		{% block pageTitle %}{{_('page.title')}}{% endblock %}
	</title>
	<meta name="keywords" content="" />
	<meta name="description" content="" />
	<meta name="wsUrl" content="{{ wsUrl }}" />

	<link rel="shortcut icon" href="{{ asset('favicon.ico') }}" type="image/x-icon"/>
	<link rel="stylesheet" href="{{ asset('main.css') }}" />
  <link href="https://fonts.googleapis.com/css?family=Open+Sans|Montserrat:400,400i,700,700i" rel="stylesheet">
</head>
<body class="{{ theme }} {{ pageClass }}">
{% include 'partials/header/header.njk' %}

{% block banner %}{% endblock %}

<div class="main-content container mobile-main-content">
	{% block main %}{% endblock %}
</div>

{% include 'partials/footer.njk' %}

<script src="{{ wsUrl }}/socket.io/socket.io.js"></script>
<script type="text/javascript" src="{{ asset('vendor.js') }}"></script>
<script type="text/javascript" src="{{ asset('main.js') }}"></script>
<script>
  (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
    (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
    m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
  })(window,document,'script','https://www.google-analytics.com/analytics.js','ga');

  ga('create', '{{ gAId }}', 'auto');
  ga('send', 'pageview');
</script>
{% block extra %}{% endblock %}

</body>

</html>
